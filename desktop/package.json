    "sharp": "^0.33.5",
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.3",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@types/react": "^18.2.52",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.0.1",
    "electron": "^31.0.1",
    "electron-builder": "^24.13.3",
    "next": "^14.2.4",
    "nextron": "^9.1.0",
    "postcss": "^8.4.38",
    "prisma": "^5.21.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3"
  },
  "build": {
    "appId": "com.suasanax.pos",
    "productName": "Sepai Studio",
    "copyright": "Copyright Â© 2025",
    "directories": {
      "output": "dist"
    },
    "files": [
      "app/**/*",
      "dist/**/*",
      "renderer/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "zip"
      ],
      "icon": "resources/sepai-icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "resources/sepai-icon.icns"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "resources/sepai-icon.jpg"
    },
    "publish": {
      "provider": "github",
      "owner": "amirun573",
      "repo": "sepAI",
      "releaseType": "draft"
    }
  }
}
