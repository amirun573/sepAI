    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "electron-log": "^5.3.0",
    "electron-serve": "^1.3.0",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.3.9",
    "express": "^4.21.0",
    "file-saver": "^2.0.5",
    "flowbite": "^2.5.1",
    "flowbite-react": "^0.10.1",
    "formidable": "^3.5.1",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "next": "14.2.7",
    "next-auth": "^4.24.7",
    "next-pwa": "^5.6.0",
    "node-cron": "^3.0.3",
    "prisma": "^5.12.1",
